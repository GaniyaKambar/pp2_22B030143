thisdict = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
print(thisdict)

thisdict1 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
print(thisdict1["brand"])

thisdict3 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964,
    "year": 2020
}
print(thisdict3)

print(len(thisdict3))

thisdict4 = {
    "brand": "Ford",
    "electric": False,
    "year": 1964,
    "colors": ["red", "white", "blue"]
}

thisdict4 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
print(type(thisdict4))

thisdict5 = dict(name="John", age=36, country="Norway")
print(thisdict5)

thisdict6 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
x = thisdict6["model"]

x2 = thisdict6.get("model")
x1 = thisdict6.keys()

car1 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}

x6 = car1.keys()

print(x)  # before the change

car1["color"] = "white"

print(x)  # after the change

x = thisdict.values()

car = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}

x4 = car.values()

print(x)  # before the change

car["year"] = 2020

print(x)  # after the change

car5 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}

x = car.values()

print(x)  # before the change

car["color"] = "red"

print(x)  # after the change

car6 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}

x = car.items()

print(x)  # before the change

car6["year"] = 2020

print(x)  # after the change

thisdict7 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
if "model" in thisdict7:
    print("Yes, 'model' is one of the keys in the thisdict dictionary")

