thisdict17.update({"color": "red"})

thisdict0 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
thisdict0.pop("model")
print(thisdict0)

thisdict00 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
thisdict00.popitem()
print(thisdict00)

thisdict000 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
del thisdict000["model"]
print(thisdict000)

thisdicte = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
thisdicte.clear()
print(thisdicte)

for x in thisdict:
    print(x)

for x in thisdict:
    print(thisdict[x])

for x in thisdict.values():
    print(x)

for x in thisdict.keys():
    print(x)

for x, y in thisdict.items():
    print(x, y)

thisdict152 = {
    "brand": "Ford",
    "model": "Mustang",
    "year": 1964
}
mydict = thisdict152.copy()
print(mydict)

